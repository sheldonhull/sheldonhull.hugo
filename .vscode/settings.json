{
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "grammarly.userWords": [
        "Localdb",
        "dbaconnection",
        "dbatools",
        "gists",
        "ldf",
        "localdb",
        "mdf",
        "pwsh",
        "docset"
    ], // no protected branches on this blog repo, as I edit and work with master directly for CI
    "branchwarnings.protectedBranches": [
        ""
    ],
    "conventionalCommits.scopes": [
        "blog",
        "build",
        "netlifycms",
        "microblog"
    ],
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.autoIndent": "advanced",
    },
    "go.inferGopath": false
}
