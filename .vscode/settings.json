{
    "[markdown]": {
        "editor.defaultFormatter": "tehnix.vscode-tidymarkdown"
    },
    "grammarly.userWords": [
        "CLI",
        "Localdb",
        "cli",
        "dbaconnection",
        "dbatools",
        "docset",
        "gists",
        "ldf",
        "localdb",
        "mdf",
        "omitempty",
        "pwsh"
    ], // no protected branches on this blog repo, as I edit and work with master directly for CI
    "branchwarnings.protectedBranches": [
        ""
    ],
    "conventionalCommits.scopes": [
        "blog",
        "build",
        "netlifycms",
        "microblog"
    ],
    "conventionalCommits.emojiFormat": "emoji",
    "conventionalCommits.lineBreak": "|",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.autoIndent": "advanced",
    },
    "go.inferGopath": false,
    "staticWebApps.appSubpath": "/",
    "staticWebApps.apiSubpath": "/",
    "staticWebApps.appArtifactSubpath": "build"
}
