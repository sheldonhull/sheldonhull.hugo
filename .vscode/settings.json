{
    "explorer.sortOrder": "modified",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    // "markdownlint.customRules": [".markdownlint.yaml"],
    "grammarly.userWords": [
        "CLI",
        "Localdb",
        "cli",
        "dbaconnection",
        "dbatools",
        "docset",
        "gists",
        "ldf",
        "localdb",
        "mdf",
        "omitempty",
        "pwsh"
    ], // no protected branches on this blog repo, as I edit and work with master directly for CI
    "branchwarnings.protectedBranches": [],
    "conventionalCommits.scopes": [
        "blog",
        "build",
        "netlifycms",
        "microblog",
        "style",
        "config",
        "docs",
        "codespaces",
        "brag",
        "forestry",
        "vscode",
        "webmentions",
        "layout",
        "archetypes",
        "rss",
        "creative",
        "100DaysOfCode",
        "mage",
        "tasks",
        "project",
        "docker"
    ],
    "conventionalCommits.emojiFormat": "emoji",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.autoIndent": "advanced",
    },
    "go.inferGopath": false,
    "staticWebApps.appSubpath": "/",
    "staticWebApps.apiSubpath": "/",
    "staticWebApps.appArtifactSubpath": "build",
    "markdown-image.coding.path": "/static/images",
    "markdown-image.local.path": "/static/images",
    "git.autoRepositoryDetection": false,
    "git.ignoredRepositories": [
        "codeit"
    ],
    "conventionalCommits.showNewVersionNotes": true,
    "git.ignoreSubmodules": true,
    "scm.repositories.visible": 1,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        ".prettierrc": true,
        ".github-inactive": true,
        ".forestry": true,
        ".foam": false,
        ".linky-note.yml": true,
        ".run": true,
        "**/renovate.json": true,
        "_*/": false,
        "diagrams": true,
        ".*.y*ml": true,
        ".git*": true,
        ".devcontainer": false,
        ".vscode": true,
        ".archive": true,
        "LICENSE": true,
        ".bhugo": true,
        ".code-config": true,
        "algolia.json": true,
        "archetypes": true,
        "archive": false,
        "artifacts": true,
        "assets": true,
        "build": true,
        "build.ps1": true,
        "config.yml": true,
        "data": true,
        "GitVersion.yml": true,
        "go-algolia": true,
        "go.mod": true,
        "go.sum": true,
        "init.ps1": true,
        "layouts": false,
        "lefthook.yml": true,
        "LICENSE.md": true,
        "main-json-builder-test.go_": true,
        "netlify.toml": true,
        "node_modules": true,
        "nodesource_setup.sh": true,
        "package-lock.json": true,
        "package.json": true,
        "public": true,
        "readme.md": true,
        "RequiredModules.psd1": true,
        "resources": true,
        "scripts": true,
        "squealer.yml": true,
        "stacks": true,
        "static": false,
        "task.build.ps1": true,
        "tools": true,
        "webmentions.json": true,
        "config": true,
        "static/css": true,
        "static/*.png": true,
        "static/*.json": true,
        "static/*.xml": true,
        "static/*.html": true,
        "static/*.ico": true,
        "static/cms": true,
        "static/javascript": true,
        "static/files": true,
        "static/admin": true
    },
    "explorerExclude.backup": null,
}
