{
    "explorer.sortOrder": "mixed",
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "grammarly.userWords": [
        "CLI",
        "Localdb",
        "cli",
        "dbaconnection",
        "dbatools",
        "docset",
        "gists",
        "ldf",
        "localdb",
        "mdf",
        "omitempty",
        "pwsh"
    ], // no protected branches on this blog repo, as I edit and work with master directly for CI
    "branchwarnings.protectedBranches": [],
    "conventionalCommits.scopes": [
        "blog",
        "build",
        "netlifycms",
        "microblog",
        "style",
        "config",
        "docs",
        "codespaces",
        "brag",
        "forestry",
        "vscode",
        "webmentions",
        "layout",
        "archetypes",
        "rss",
        "creative"
    ],
    "conventionalCommits.emojiFormat": "emoji",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.autoIndent": "advanced",
    },
    "go.inferGopath": false,
    "staticWebApps.appSubpath": "/",
    "staticWebApps.apiSubpath": "/",
    "staticWebApps.appArtifactSubpath": "build",
    "markdown-image.coding.path": "/static/images",
    "markdown-image.local.path": "/static/images",
    "git.autoRepositoryDetection": false,
    "git.ignoredRepositories": [
        "codeit"
    ],
    "conventionalCommits.showNewVersionNotes": true,
    "git.ignoreSubmodules": true,
    "scm.repositories.visible": 1,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "explorerExclude.backup": null
}
