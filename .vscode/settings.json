{
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.fontSize": 14,
    "editor.lineHeight": 26
  },
  "grammarly.files.include": [
    "*.md"
  ],
  // "markdownlint.customRules": [".markdownlint.yaml"],
  "grammarly.userWords": [
    "CLI",
    "Localdb",
    "cli",
    "dbaconnection",
    "dbatools",
    "docset",
    "gists",
    "ldf",
    "localdb",
    "mdf",
    "omitempty",
    "pwsh"
  ], // no protected branches on this blog repo, as I edit and work with master directly for CI
  "branchwarnings.protectedBranches": [],
  "conventionalCommits.scopes": [
    "blog",
    "build",
    "netlifycms",
    "microblog",
    "style",
    "config",
    "docs",
    "codespaces",
    "brag",
    "forestry",
    "vscode",
    "layout",
    "archetypes",
    "rss",
    "creative",
    "100DaysOfCode",
    "mage",
    "tasks",
    "project",
    "docker",
    "deps",
    "go",
    "precommit"
  ],
  "conventionalCommits.emojiFormat": "emoji",
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.autoIndent": "advanced",
  },
  "go.inferGopath": false,
  "staticWebApps.appSubpath": "/",
  "staticWebApps.apiSubpath": "/",
  "staticWebApps.appArtifactSubpath": "build",
  "markdown-image.coding.path": "/static/images",
  "markdown-image.local.path": "/static/images",
  "git.autoRepositoryDetection": false,
  "git.ignoredRepositories": [
    "codeit"
  ],
  "conventionalCommits.showNewVersionNotes": true,
  "git.ignoreSubmodules": true,
  "scm.repositories.visible": 1,
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    ".prettierrc": false,
    ".github-inactive": false,
    ".forestry": false,
    ".foam": false,
    ".linky-note.yml": false,
    ".run": false,
    "**/renovate.json": false,
    "_*/": false,
    "diagrams": false,
    ".*.y*ml": false,
    ".git*": false,
    ".devcontainer": false,
    ".vscode": false,
    ".archive": false,
    "LICENSE": false,
    ".bhugo": false,
    ".code-config": false,
    "algolia.json": false,
    "archetypes": false,
    "archive": false,
    "artifacts": false,
    "assets": false,
    "build": false,
    "build.ps1": false,
    "config.yml": false,
    "data": false,
    "GitVersion.yml": false,
    "go-algolia": false,
    "go.mod": false,
    "go.sum": false,
    "init.ps1": false,
    "layouts": false,
    "lefthook.yml": false,
    "LICENSE.md": false,
    "main-json-builder-test.go_": false,
    "netlify.toml": false,
    "node_modules": false,
    "nodesource_setup.sh": false,
    "package-lock.json": false,
    "package.json": false,
    "public": false,
    "readme.md": false,
    "RequiredModules.psd1": false,
    "resources": false,
    "scripts": false,
    "squealer.yml": false,
    "stacks": false,
    "static": false,
    "task.build.ps1": false,
    "tools": false,
    "config": false,
    "static/css": false,
    "static/*.png": false,
    "static/*.json": false,
    "static/*.xml": false,
    "static/*.html": false,
    "static/*.ico": false,
    "static/cms": false,
    "static/javascript": false,
    "static/files": false,
    "static/admin": false,
    "**/yarn.lock": false,
    "**/archive": false
  },
  "explorerExclude.backup": null,
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.startCommand": "hugo serve  --verbose --enableGitInfo -d _site --buildFuture --buildDrafts --gc --cleanDestinationDir",
  "frontMatter.preview.host": "http://localhost:1313",
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file",
      "pattern": "content/posts/2023/**/*.md"
    }
  ],
}