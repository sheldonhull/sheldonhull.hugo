{
    "explorer.sortOrder": "mixed",
    "[markdown]": {
        "editor.defaultFormatter": "tehnix.vscode-tidymarkdown"
    },
    "grammarly.userWords": [
        "CLI",
        "Localdb",
        "cli",
        "dbaconnection",
        "dbatools",
        "docset",
        "gists",
        "ldf",
        "localdb",
        "mdf",
        "omitempty",
        "pwsh"
    ], // no protected branches on this blog repo, as I edit and work with master directly for CI
    "branchwarnings.protectedBranches": [],
    "conventionalCommits.scopes": [
        "blog",
        "build",
        "netlifycms",
        "microblog",
        "style",
        "config",
        "add tls1.2 for older servers for ssm agent install command",
        "docs",
        "codespaces",
        "brag"
    ],
    "conventionalCommits.emojiFormat": "emoji",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.autoIndent": "advanced",
    },
    "go.inferGopath": false,
    "staticWebApps.appSubpath": "/",
    "staticWebApps.apiSubpath": "/",
    "staticWebApps.appArtifactSubpath": "build",
    "markdown-image.coding.path": "/static/images",
    "markdown-image.local.path": "/static/images"
}
