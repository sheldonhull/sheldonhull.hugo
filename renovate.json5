{
  extends: [
    'github>sheldonhull/renovate:renovate.json5',
    'github>aquaproj/aqua-renovate-config:installer-script#2.8.0(scripts/.*\\.sh)',
    'github>aquaproj/aqua-renovate-config:aqua-renovate-config#2.8.0(default\\.json)',
  ],
  packageRules: [
    {
      matchPackageNames: [
        // Netlify has go version set and if this mismatches can cause failures in builds like:
        // `compile: version "go1.18.7" does not match go tool version "go1.19.3"`
        'golang/go',

      ],
      enabled: false,
    },
  ],
}
